r_has_part(PLAT, ALIMENT) //PLAT contient ALIMENT (pain/farine, céréale/blé, ...)
r_holo(X,Y)

r_is_a(X,Y) //X est une instance de Y (beurre/aliment, eau/liquide, ...)
r_hypo(X,Y) //

r_syn(X,Y) //X interchangeable avec Y ()

r_lieu(PLAT,LIEU) //origine de...

r_instr(VERBE/ACTION, OBJET/OUTIL) // spécifier instrument d'ue acotion

r_carac(PLAT/ALIMENT, ADJECTIF) // spécifier une caractéristique
